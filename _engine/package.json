{
  "name": "game",
  "version": "0.0.1",
  "description": "Moddable 2D Multiplayer Game Engine Built on Nengi and PixiV5",
  "scripts": {
    "start": "concurrently \"npm run start:client\" \"npm run start:server\"",
    "start:client": "node -r esm ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --config webpack.dev.js",
    "start:server": "nodemon --inspect ./server/index.js --ignore public/",
    "build": "node -r esm ./node_modules/webpack/bin/webpack --config webpack.prod.js"
  },
  "author": "Randy Lebeau <github/supremetechnopriest>",
  "license": "MPL-2.0",
  "dependencies": {
    "@modngn/client": "file:../../../core-client",
    "@modngn/server": "file:../../../core-server",
    "@modngn/plugin-public-files": "file:../../../plugin-public-files",
    "esm": "^3.2.25"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "html-webpack-plugin": "^3.2.0",
    "terser-webpack-plugin": "^1.3.0",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.1"
  }
}
