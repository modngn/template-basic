{
  "name": "{{name}}",
  "version": "0.0.1",
  "description": "Moddable 2D Multiplayer Game Built on ModNGN",
  "private": true,
  "scripts": {
    "start": "concurrently \"npm run start:client\" \"npm run start:server\"",
    "start:client": "node -r esm ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --config webpack.dev.js",
    "start:server": "nodemon --inspect ./server/index.js --ignore public/",
    "build": "node -r esm ./node_modules/webpack/bin/webpack --config webpack.prod.js"
  },
  "author": "{{user}}",
  "license": "MPL-2.0",
  "dependencies": {
    "@modngn/client": "0.0.x",
    "@modngn/server": "0.0.x",
    "@modngn/plugin-public-files": "0.0.x"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "esm": "^3.2.25",
    "get-port": "^5.0.0",
    "html-webpack-plugin": "^3.2.0",
    "terser-webpack-plugin": "^1.3.0",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.1"
  }
}
